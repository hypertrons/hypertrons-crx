    "tslint-loader": "^3.5.4",
    "typescript": "^4.2.2",
    "web-ext": "^5.5.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "3.2.1",
    "write-file-webpack-plugin": "^4.5.1"
