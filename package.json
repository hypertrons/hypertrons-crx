    "@hot-loader/react-dom": "^16.13.0",
    "@octokit/core": "^3.4.0",
    "@types/chrome": "0.0.104",
    "@types/react": "^17.0.2",
    "copy-to-clipboard": "^3.3.1",
    "delegated-events": "^1.1.2",
    "echarts": "^5.0.2",
    "github-url-detection": "^4.10.0",
    "jquery": "^3.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-test-renderer": "^17.0.1"
