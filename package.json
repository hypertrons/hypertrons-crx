    "prettier": "prettier --write '**/*.{js,jsx,css,html}'"
  },
  "dependencies": {
    "@antv/graphin": "^2.0.6",
    "@antv/graphin-components": "^2.0.7",
    "@fluentui/react": "^7.121.1",
